      },
      "source": [
        "data = [1, 2, 3, 4, 5, 6, 7, 8]\n",
        "\n",
        "def sequential_search(data, key):\n",
        "    tmp = [0] * (len(data) + 1)    #設一個空陣列，長度為data長度+1\n",
        "    for i in range(1, len(data)):  #將data的值給到tmp\n",
        "        tmp[i] = data[i - 1]\n",
        "    tmp[0] = key                   \n",
        "    i = len(data)                  #設定索引並開始比較\n",
        "    while tmp[i] != tmp[0]:\n",
        "        i -= 1\n",
        "    return i - 1                   \n",
        "\n",
        "index = sequential_search(data, 3)\n",
        "if index >= 0:\n",
        "    print(\"you can find the index: \" + str(index))\n",
        "else:\n",
        "    print(\"you can't find the index\")"
      ],
      "execution_count": 7,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
            "find the index: 2\n"
          ],
          "name": "stdout"
        }
      ]
    }
  ]
